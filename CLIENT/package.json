        "prettier": "^2.1.2",
        "pretty-quick": "^3.1.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "pre-push": "npm test"
        }
    },
    "postcss": {
        "syntax": "postcss-scss",
        "map": false,
        "plugins": {
            "css-declaration-sorter": {
                "order": "smacss"
            }
        }
    },
    "postcssSorting.config": {
        "order": [
            "custom-properties",
            "dollar-variables",
            "declarations",
            "at-rules",
            "rules"
        ],
        "properties-order": [
            "display",
            "position",
            "top",
            "right",
            "bottom",
            "left"
        ]
